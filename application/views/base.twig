<!doctype html>
<html class="no-js" lang="en">

<head>
  {% block head %}
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>E.A.T. Challenge</title>
  <link rel="stylesheet" href="{{ baseURL }}/application/assets/css/app.css">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/foundicons/3.0.0/foundation-icons.css" rel="stylesheet" type="text/css">
  {% endblock %}
</head>

<body>

  <div class="title-bar" data-responsive-toggle="EatMenu" data-hide-for="small">
    <button class="menu-icon" type="button" data-toggle></button>
    <div class="title-bar-title"><a href="{{ baseURL }}">E.A.T. Challenge</a></div>
  </div>
  <div class="top-bar" id="EatMenu">
    <div class="top-bar-left">
      <ul class="menu" data-responsive-menu="accordion">
        <li class="menu-text"><a href="{{ baseURL }}">E.A.T. Challenge</a></li>
        <li><a href="#">Form</a></li>
      </ul>
    </div>
    <!--
    <div class="top-bar-right">
      <ul class="menu">
        <li><a href="#">My Account</a></li>
        <li><a class="button">Login</a></li>
      </ul>
    </div>
  -->
  </div>

  {% block content %} {% endblock %}

  <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
  <script src="https://dhbhdrzi4tiry.cloudfront.net/cdn/sites/foundation.js"></script>
  <script>
    $(document).foundation();
  </script>
  <script type="text/javascript" src="https://intercom.zurb.com/scripts/zcom.js"></script>

  {% block scriptBlock %}{% endblock%}

</body>

</html>
